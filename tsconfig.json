{
  "compilerOptions": {
    "target": "esnext",
    "baseUrl": ".",
    "module": "esnext",
    "strict": false,
    "jsx": "preserve",
    "moduleResolution": "node",
    "allowJs": true,
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
  },
  "include": [
    "docs/**/*.ts",
    "docs/**/*.d.ts",
    "docs/**/*.tsx",
    "docs/**/*.vue",
    "docs/.vitepress/data/configSchema.ts"
  ],
  "types": [
    "element-plus/global"
  ],
  "paths": {
    "@components": [
      "docs/.vitpress/components"
    ],
    "@utils": [
      "docs/.vitpress/utils"
    ],
    "@store": [
      "docs/.vitpress/store"
    ]
  },
}
